{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/editor",
      "dest": "/index.html"
    },
    {
      "src": "/editor/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard/blogs",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard/notifications",
      "dest": "/index.html"
    },
    {
      "src": "/settings/edit-profile",
      "dest": "/index.html"
    },
    {
      "src": "/settings/change-password",
      "dest": "/index.html"
    },
    {
      "src": "/signin",
      "dest": "/index.html"
    },
    {
      "src": "/signup",
      "dest": "/index.html"
    },
    {
      "src": "/search/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/user/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/index.html"
    }
  ]
}
